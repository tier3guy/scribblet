{
    "singleQuote": true,
    "semi": true,
    "trailingComma": "all",
    "jsxSingleQuote": true,
    "printWidth": 100,
    "tabWidth": 4,
    "useTabs": false,
    "bracketSpacing": true,
    "arrowParens": "always",
    "overrides": [
        {
            "files": "*.json",
            "options": {
                "printWidth": 80
            }
        },
        {
            "files": "*.md",
            "options": {
                "printWidth": 80
            }
        }
    ]
}
